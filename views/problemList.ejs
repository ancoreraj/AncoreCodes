<%- include('header'); %>
    <div class="main">
        <h3 class="text-center text-danger">Search Results for "<%=searchQuery%>"</h3>
        <%if(docs.length > 0){%>
            <%docs.forEach((doc)=>{%>
                <div class="results m-3">
                    <a href="/problem/<%=doc._id%>">
                        <h4 class="text-center">
                            <%=doc.title%>
                        </h4>
                    </a>
                </div>
        <%})}else{%>
            <h3 class="text-center">Sorry, no results found.</h3>
        <%}%>


    </div>

    <%- include('footer'); %>